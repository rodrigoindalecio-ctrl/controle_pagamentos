# GEMINI_API_KEY: Required for Gemini AI API calls.
GEMINI_API_KEY="MY_GEMINI_API_KEY"

# APP_URL: The URL where this applet is hosted.
APP_URL="MY_APP_URL"

# SUPABASE - Banco de Dados
# Obtidos em: https://supabase.com/dashboard/project/SEU_PROJECT_ID/settings/api
SUPABASE_URL="https://SEU_PROJECT_ID.supabase.co"

# Chave pública (pode ser exposta no front-end)
SUPABASE_ANON_KEY="sb_publishable_..."

# Chave de serviço (NUNCA expor no front-end! Apenas no backend/servidor)
# Obtida em: Project Settings > API > service_role (Secret)
SUPABASE_SERVICE_ROLE_KEY="sb_secret_..."
